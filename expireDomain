_HOST="$1"
EXPIREDOMAIN=`whois $_HOST |grep expires | sed 's/ //g' |cut -d: -f2`
#echo $EXPIREDOMAIN
EXPIREDATE=$(date --date $EXPIREDOMAIN +%s)
echo $EXPIREDATE

# Este script depende do comando whois instalado no servidor Zabbix
